# Floating button used for creating new encyrption key

<FloatingButton@MDFloatingBottomButton+MDTooltip>:
    name: "FloatingButton"
    tooltip_text: "Generate new pair of keys"
    id: floating_button_id
    md_bg_color: app.theme_cls.primary_color
    tooltip_bg_color: app.theme_cls.primary_color
    icon: "plus"
    on_release: app.new_key_pair_click()
    shift_left: 100
    shift_y: 45


